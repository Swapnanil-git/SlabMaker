@model IEnumerable<SlabMaker.Models.Order>

<h2>Received Orders</h2>

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Grade</th>
            <th>Length</th>
            <th>Width</th>
            <th>Quantity</th>
            <th>Weight</th>
            <th>Received Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.Grade</td>
                <td>@order.Length</td>
                <td>@order.Width</td>
                <td>@order.Quantity</td>
                <td>@order.Weight</td>
                <td>@order.Status</td>
                <td>@order.ReceivedDate</td>
            </tr>
        }
    </tbody>
</table>
